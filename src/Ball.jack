class Ball
{
    field int xCord, yCord, size;
    field int xSpeed, ySpeed;

    constructor Ball new()
    {
        let xCord = 10;
        let yCord = 10;
        let size = 5;
        let xSpeed = 2;
        let ySpeed = 2;

        return this;

    }

    method void draw() {
        do Screen.drawRectangle(xCord - size, yCord - size, xCord + size, yCord + size);
        return;
    }
}